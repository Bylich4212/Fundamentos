#include <iostream>
#include <math.h>
#include <iomanip.h>

float vol_cilindro(float r, float a);// cabecera de la funcion 
using namespace std;
int main() {
	float radio, altura, volumen;
	cout << "ingrese el radio: "; cin >> radio;
	cout << " ingresa la altura: "; cin >> altura;
	volumen = vol_cilindro(radio, altura);// la llamada a la funcion 
